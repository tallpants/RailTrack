{
  "name": "railtrack",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "cross-env NODE_ENV=production node dist/index",
    "dev": "nodemon",
    "test": "jest",
    "coverage":
      "opener coverage/lcov-report/index.html || echo 'Run tests first'"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js|ts)$",
    "moduleFileExtensions": ["ts", "js"],
    "collectCoverage": true,
    "collectCoverageFrom": ["**/*.ts"],
    "coverageReporters": ["lcov", "text"],
    "verbose": true
  },
  "dependencies": {
    "actions-on-google": "^1.10.0",
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "dotenv": "^5.0.1",
    "express": "^4.16.3"
  },
  "devDependencies": {
    "@types/actions-on-google": "^1.8.0",
    "@types/axios": "^0.14.0",
    "@types/dotenv": "^4.0.2",
    "@types/express": "^4.11.1",
    "@types/jest": "^22.2.3",
    "@types/node": "^9.6.1",
    "cross-env": "^5.1.4",
    "jest": "^22.4.3",
    "nodemon": "^1.17.3",
    "opener": "^1.4.3",
    "source-map-support": "^0.5.4",
    "ts-jest": "^22.4.4",
    "ts-node": "^5.0.1",
    "typescript": "^2.8.1"
  }
}
